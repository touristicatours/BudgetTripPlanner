{
  "name": "tripweaver",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.0",
    "prettier": "^3.0.0",
    "turbo": "^1.10.0"
  },
  "packageManager": "npm@9.0.0",
  "dependencies": {
    "next": "^14.0.0"
  }
}
